def ExOh(str)

  # code goes here
  array = str.split(//)
  num_of_O = 0
  num_of_X = 0
  (0..array.length).each do |i|
    if array[i] == "o"
      num_of_O += 1
    elsif array[i] == "x"
      num_of_X += 1
    end
  end
  
  if num_of_O == num_of_X
    return true
  else 
    return false
  end
end
   
# keep this function call here 
# to see how to enter arguments in Ruby scroll down   
ExOh(STDIN.gets)
